#!/usr/bin/env zsh
alias sirb="irb --simple-prompt"
alias gotocode="cd ~/Documents/Code/"

alias gc="git commit -m"
alias gp="git push"
alias gpl="git pull"
alias gch="git checkout"
alias ga="git add"
alias gs="git status"
alias gd="git difftool"
alias gds="git difftool --staged"
alias gl="git log --stat"

alias dst="docker start"
alias dsp="docker stop"
alias dps="docker ps"
alias dr="docker run"
alias de="docker exec -t -i /bin/zsh"
alias dl="docker logs --tail 15"
